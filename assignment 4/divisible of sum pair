#include <iostream>
#include<vector>
using namespace std;
int main()
{
    int n,k,i,j,sum,count=0;
    cin>>n>>k;
    int arr[n];
    for(i=0;i<n;i++)
    cin>>arr[i];
    
    
       for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++) {
            sum=arr[i]+arr[j];
            if(sum%k==0)
                count++;
                
            
        }
         }
    cout<<count;
return 0;
}
